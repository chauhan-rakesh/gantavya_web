<%- include('../layouts_admin/admin') %>

<div class="col-xs-12">
          <div class="box">
            <div class="box-header">
              <h3 class="box-title">Students Registered</h3>

              <div class="box-tools">
                <div class="input-group input-group-sm" style="width: 150px;">
                  <input type="text" name="table_search" class="form-control pull-right" placeholder="Search">

                  <div class="input-group-btn">
                    <button type="submit" class="btn btn-default"><i class="fa fa-search"></i></button>
                  </div>
                </div>
              </div>
            </div>
            <!-- /.box-header -->
            <div class="box-body table-responsive no-padding">
              <table class="table table-hover">
                <tr>
                  <th>ID</th>
                  <th>Name</th>
                  <th>Branch</th>
                  <th>Year</th>
                  
                  <th>College</th>
                  <th>Contact no</th>
                  <th>Roll no</th>

                  <th>email</th>
                  <th>password</th>
                </tr>
            <% student.forEach(function(student){  %>
                <tr>

                  <td><%= student._id %></td>
                  <td><%= student.studentName %></td>
                  <td><%= student.branch %></td>
                  <td><%= student.year %></td>

                  <td><%= student.college %></td>
                  <td><%= student.ContactNo %></td>
                  <td><%= student.Roll_no %></td>

                  <td><%= student.email %></td>
                  <td><%= student.password %></td>
                  <td><a href="student/edit/<%= student._id %>" class="btn btn-info"><i class="fa fa-pencil" aria-hidden="true"></i></a></td>
                  <td>
                      <form action="/admin/student/<%= student._id %>?_method=DELETE" method="post">
                        <button type="submit" class="btn btn-danger"><i class="fa fa-trash" aria-hidden="true"></i></button>
                      </form>

                  </td>
                        <% }) %>
              </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
      </div>
    </section>
    <!-- /.content -->
  </div>




<%- include('../layouts_admin/footer') %>
